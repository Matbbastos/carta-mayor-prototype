{
  "formatters": {
    "brief": {
      "datefmt": "%Y-%m-%d %H:%M:%S",
      "format": "%(asctime)s    %(levelname)-8.8s - %(filename)-20s: %(message)s"
    }
  },
  "handlers": {
    "info_console": {
      "class": "logging.StreamHandler",
      "formatter": "brief",
      "level": "DEBUG"
    }
  },
  "root": {
    "handlers": [
      "info_console"
    ],
    "level": "DEBUG"
  },
  "version": 1
}
